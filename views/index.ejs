<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Bincom Test Election App</title>
    <link rel="stylesheet" href="/css/style.css">
    <script src="/send_url.js"></script>
  </head>
  <body>
    <header>
      <a href="/" class="header-logo">Election Results</a>
    </header>
    <div class="container">
      <div class="container-header">
        <h1>Polling Units (PU)</h1>
      </div>
      <div class="index-table-wrapper">
        <div class="table-head">
          <span class="id-column">ID</span>
          <span class="id-column">PU</span>
          <span class="id-column">Result</span>
        </div>
        <!-- Delete the items constant -->
        <% const items = [
          {id: 1, name: 'Rano', result: 908},
          {id: 2, name: 'Ghari', result: 1028},
          {id: 3, name: 'Gote', result: 2908},
        ] %>
        <!-- Delete up until here -->
        <ul class="table-body">
          <% items.forEach((item) => { %>
            <li>
              <div class="item-data">
                <span class="id-column"><%= item.id %></span>
                <span class="id-column"><%= item.name %></span>
                <span class="id-column"><%= item.result %></span>
              </div>
            </li>
          <% }) %>
        </ul>
      </div>
    </div>
  </body>
</html>